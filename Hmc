#include<stdio.h>
int main()
{
	int a[32],b[38],i,j,k,count=1,c[32],d[38],z;
	printf("enter sender data:");
	for(i=1;i<=32;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=1;i<=38;i++)
	{
		b[i]=0;
	}
	b[1]=b[2]=b[4]=b[8]=b[16]=b[32]=-1;
	for(i=1;i<=38;i++)
	{
		if(b[i]!=-1)
		{
			b[i]=a[count];
			count++;
		}
	}
	b[1]=b[3]^b[5]^b[7]^b[9]^b[11]^b[13]^b[15]^b[17]^b[19]^b[21]^b[23]^b[25]^b[27]^b[29]^b[31]^b[33]^b[35]^b[37];
	b[2]=b[3]^b[6]^b[7]^b[10]^b[11]^b[14]^b[15]^b[18]^b[19]^b[22]^b[23]^b[26]^b[27]^b[30]^b[31]^b[34]^b[35]^b[38];
	b[4]=b[5]^b[6]^b[7]^b[12]^b[13]^b[14]^b[15]^b[20]^b[21]^b[22]^b[23]^b[28]^b[29]^b[30]^b[31]^b[36]^b[37]^b[38];
	b[8]=b[9]^b[10]^b[11]^b[12]^b[13]^b[14]^b[15]^b[24]^b[25]^b[26]^b[27]^b[28]^b[29]^b[30]^b[31];
	b[16]=b[17]^b[18]^b[19]^b[20]^b[21]^b[22]^b[23]^b[24]^b[25]^b[26]^b[27]^b[28]^b[29]^b[30]^b[31];
	b[32]=b[33]^b[34]^b[35]^b[36]^b[37]^b[38];
	for(i=1;i<=38;i++)
	{
		d[i]=b[i];
		printf("%d ",d[i]);
	}
	printf("enter reciever data:");
	for(i=1;i<=38;i++)
	{
		scanf("%d",&c[i]);
	}
	k=0;
	for(i=1;i<=38;i++)
	{
		if(i==1||i==2||i==4||i==8||i==16||i==32)
		{
			continue;
		}
		else
		{
			if(d[i]^c[i]==1)
			{
			  z=i;
			  c[i]=d[i];
			  break;
			}
		}
   }
   printf("the error is at %d position:",z);
   printf("after dhula:");
   
   for(i=1;i<=38;i++)
   {
   	printf("%d ",c[i]);
   }
}
